{
  "extends": ["@trybe/eslint-config-frontend"],
  "rules": {
    "react/jsx-one-expression-per-line": "off",
    "max-len": ["error", {
      "code": 160
    }]
  },
  "overrides": [
    {
      "files": ["src/**/*.{js,jsx}"],
      "rules": {
        "react/jsx-max-depth": ["error", {
          "max": 7
        }]
      }
    }
  ]
}
